    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "XuTL\\Passport\\Sms\\SmsLoginGrantProvider"
      ]
    }
  },
  "prefer-stable": true

{
  "name": "xutl/laravel-passport-sms",
  "description": "This is a Laravel passport grant for the SMS.",
  "keywords": [ "passport", "laravel", "sms" ],
  "type": "library",
  "license": "MIT",
  "require": {
     "xutl/laravel-sms": "^1.0"
  },
  "autoload": {
    "psr-4": {
